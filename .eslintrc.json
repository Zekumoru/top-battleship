{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "lines-between-class-members": "off",
    "no-param-reassign": "off",
    "no-shadow": "off",
    "no-use-before-define": "off",
    "func-names": "off",
    "no-plusplus": "off",
    "class-methods-use-this": "off",
    "no-new": "off",
    "consistent-return": "off",
    "max-len": "off",
    "no-continue": "off"
  }
}
